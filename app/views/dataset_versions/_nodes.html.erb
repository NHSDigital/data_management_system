<div id="data-items-panel">
  <table class="table table-hover" id="data_source_items_table">
    <thead>
      <tr>
        <th width="10">Type</th>
        <th width="15%">Name</th>
        <th width="15%">Annotation/Data Dictionary Name</th>
        <th width="30%">Description</th>
        <th width="3%">Min Occurs</th>
        <th width="3%">Max Occurs</th>
        <th width="10%">Governance</th>
        <th width="4%">Child nodes</th>
        <th width="10%"></th>
      </tr>
    </thead>
    <tbody>
      <% node = @dataset_version.version_entity %>
      <%= render 'nodes/details', node: node %>
      <%= render 'nodes/child_node_details', node: node %>
    </tbody>
  </table>
</div>
