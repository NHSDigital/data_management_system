<div class="row">
  <div class='col-sm-10 col-sm-offset-1'>
    <h1>View CAS Application</h1>
    
    <p><%= link_to 'All applicaitons', cas_applications_path %></p>
    <p><%= link_to 'Edit this applicaiton', edit_cas_application_path(@cas_application) %></p>

    <%= bootstrap_form_for(@cas_application, horizontal: true) do |form| %>
      <%= form.error_and_warning_alert_boxes %>
      <% form.readonly = true %>

      <%= render partial: 'form', locals: { form: form } %>
    <% end %>
  </div>
</div>
