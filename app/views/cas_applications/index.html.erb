<div class="row">
  <div class='col-sm-10 col-sm-offset-1'>
    <h1>CAS Applications List</h1>
    <p><%= link_to 'New applicaitons', new_cas_application_path %></p>

    <table class="table">
      <tr>
        <th>ID</th>
        <th>Edit</th>
        <th>Show</th>
      </tr>
      <% CasApplication.all.each do |ca| %>
        <tr>
          <td><%= ca.id %></td>
          <td><%= link_to 'Edit', edit_cas_application_path(ca) %></td>
          <td><%= link_to 'Show', cas_application_path(ca) %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>
