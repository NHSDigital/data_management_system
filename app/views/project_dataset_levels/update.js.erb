<%
  @project ||= @project_dataset_level.project # shallow routing means this may not have been set
%>

$('#cas_project_dataset_levels_tables').html("<%= escape_javascript(render partial: 'projects/cas_project_dataset_levels', locals: { project: @project } ) %>");
$("#approve_unactioned_datasets").load(" #approve_unactioned_datasets > *")

$('#dataset_approval_status').html("<%= escape_javascript approval_button_message(@project_dataset_level.approved) %>");
$('#dataset_approval_status').removeClass().addClass('label label-<%= approval_button_style(@project_dataset_level.approved) %>');

$('#status_label').html("<%= escape_javascript(render partial: 'projects/status_label', locals: { project: @project } ) %>");
